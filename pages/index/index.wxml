<view class="container">
  <view class="drag" bindtouchmove="touchMoveChange" bindtap="touchStart"  style="left:{{drag_style.x}};top:{{drag_style.y}};">
    <image src="/pages/images/template.png" class="img-style" animation="{{animTopOne}}" bindtap="collect"></image>
    <image src="/pages/images/circle.png" class="img-style" animation="{{animInput}}" bindtap="input"></image>
    <image src="/pages/images/square.png" class="img-style" animation="{{animTranspond}}" bindtap="transpond"></image>
    <image src="/pages/images/text1.png" class="img-style" animation="{{animCollect}}" bindtap="collect"></image>
    <image src="/pages/images/1.png" class="img-plus-style" animation="{{animPlus}}" bindtap="plus"></image>
  </view>
</view>